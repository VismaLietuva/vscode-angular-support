{
  "name": "vscode-angular-support",
  "displayName": "Angular Support",
  "description": "Go to / peek angular specific definitions",
  "version": "0.1.2",
  "publisher": "vismalietuva",
	"icon": "images/icon.svg",
  "repository": {
    "type": "git",
    "url": "https://github.com/VismaLietuva/vscode-angular-support"
  },
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:html"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "typescript": "^2.0.3"
  },
  "devDependencies": {
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}